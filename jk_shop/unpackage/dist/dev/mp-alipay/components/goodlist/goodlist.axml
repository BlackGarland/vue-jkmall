<view class="goods_list"><view data-event-opts="{{[['tap',[['itemClick',['$0'],[[['goods','id',item.id]]]]]]]}}" class="goods_item" onTap="__e" a:for="{{goods}}" a:for-item="item" a:for-index="__i0__" a:key="id"><image src="{{item.img_url}}"></image><view class="price"><text>{{item.sell_price}}</text><text>{{item.market_price}}</text></view><view class="name">{{item.title}}</view></view></view>